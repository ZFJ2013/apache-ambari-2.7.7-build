<p>{{{popupBody}}}</p>
<div>
  <table>
    <thead>
      <tr>
        <th></th>
        <th><strong>{{view.stackToVersion}}</strong></th>
        <th></th>
        <th><strong>{{view.stackFromVersion}}</strong></th>
      </tr>
    </thead>
    <tbody>
      {{#each service in view.servicesToBeReverted}}
        <tr>
          <td>{{service.displayName}}</td>
          <td class="service-version-info">
            <span class="label available">
              {{service.toVersion}}
            </span>
          </td>
          <td><i class="icon-arrow-left"></i></td>
          <td class="service-version-info">
            <span class="label available">
              {{service.fromVersion}}
            </span>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
